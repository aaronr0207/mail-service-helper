        "illuminate/support": "^11 || ^12.0",
        "illuminate/view": "^11 || ^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aaronr0207\\MailServiceHelper\\MailServiceHelperServiceProvider"
            ]
