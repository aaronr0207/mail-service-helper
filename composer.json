    "description": "Helper para enviar correos electrónicos haciendo uso de un microservicio externo a través de GuzzleHttp",
    "type": "library",
    "version": "1.12.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Aaronr0207\\MailServiceHelper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Aarón Romero"
        }
    ],
    "minimum-stability": "stable",
    "require": {
